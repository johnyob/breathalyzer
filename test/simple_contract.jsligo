type s = int;
type ret = [list<operation>, s];

@entry
const increment = (delta : int, store : s) : ret =>
  [list([]), store + delta];

@entry
const decrement = (delta : int, store : s) : ret =>
  [list([]), store - delta];

@entry
const reset = (_ : unit, _ : s) : ret =>
  [list([]), 0];

@view
const get_value = (_ : unit, s : s) : s =>
  s
